package components

import (
    "github.com/k23dev/natalianatalia/app/models"
)

templ SelectorScapp(list []models.Scapp){

<label class="text-gray-700" for="item_id">
    Tanga
    <select name="scapp_id" id="scapp_id" class="block px-3 py-2 bg-white border border-gray-300 rounded-md shadow-sm w-52 focus:outline-none focus:ring-primary-500 focus:border-primary-500">
    for _, item :=range(list){
        <option value={item.GetIDAsString()}>{item.Name}</option>
    }
    </select>
</label>
}
